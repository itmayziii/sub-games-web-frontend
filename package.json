{
  "name": "sub-games-companion-web-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "analyze": "source-map-explorer 'build/static/js/*.js' --html result.html",
    "start": "HTTPS=true SSL_CRT_FILE=local-sub-games-com.crt SSL_KEY_FILE=local-sub-games-com.key HOST=local.sub-games.com craco start",
    "build": "craco build",
    "test": "craco test",
    "eject": "react-scripts eject",
    "relay": "relay-compiler --schema schema.graphql --persist-output ./src/__generated__/queryMap.json --language typescript --src ./src/ --watchman false $@",
    "generate:schema": "npx get-graphql-schema http://localhost:4000/graphql > schema.graphql"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@craco/craco": "~6.1.2",
    "@material-ui/core": "~4.11.4",
    "@material-ui/icons": "~4.11.2",
    "@testing-library/jest-dom": "~5.11.4",
    "@testing-library/react": "~11.1.0",
    "@testing-library/user-event": "~12.1.10",
    "@types/jest": "~26.0.15",
    "@types/js-cookie": "~2.2.6",
    "@types/node": "~12.0.0",
    "@types/react": "~17.0.3",
    "@types/react-dom": "~17.0.3",
    "@types/react-relay": "~11.0.1",
    "@types/react-router-dom": "~5.1.7",
    "@types/sha.js": "~2.4.0",
    "@types/tailwindcss": "~2.0.2",
    "@typescript-eslint/eslint-plugin": "~4.22.0",
    "autoprefixer": "~9.8.6",
    "babel-plugin-relay": "~11.0.2",
    "classnames": "~2.3.1",
    "eslint": "~7.24.0",
    "eslint-config-standard-with-typescript": "~20.0.0",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-node": "~11.1.0",
    "eslint-plugin-promise": "~4.3.1",
    "graphql": "~15.5.0",
    "js-cookie": "~2.2.1",
    "jwt-decode": "~3.1.2",
    "postcss": "~7.0.35",
    "react": "0.0.0-experimental-a15586001",
    "react-dom": "0.0.0-experimental-a15586001",
    "react-relay": "~11.0.2",
    "react-router-dom": "~5.2.0",
    "react-scripts": "4.0.3",
    "react-spinners": "~0.10.6",
    "recoil": "~0.2.0",
    "relay-compiler": "~11.0.2",
    "relay-compiler-language-typescript": "~14.0.0",
    "relay-runtime": "~11.0.2",
    "sha.js": "~2.4.11",
    "source-map-explorer": "~2.5.2",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@~2.1.2",
    "typescript": "^4.1.2"
  },
  "devDependencies": {}
}
